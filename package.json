{
  "name": "ofx4js",
  "version": "2.1.0",
  "description": "A javascript OFX library, ported from OFX4J",
  "keywords": [
    "ofx",
    "banking",
    "finance"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "debug": "^4.1.1",
    "sax": "^1.2.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/assert": "^1.4.1",
    "@types/debug": "0.0.31",
    "@types/jest": "^23.3.10",
    "@types/sax": "^1.0.1",
    "@types/uuid": "^3.4.4",
    "jest": "^29.3.1",
    "rollup": "^4.53.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^7.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "start": "npm run test && npm run build && npm run typedoc",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "typedoc": "typedoc --name Ofx4js --readme Readme.md --excludeExternals --includeDeclarations --target ES5 --module commonjs --theme minimal --out doc/ src/"
  },
  "author": "Andrew Olson <andrew@olsononline.org>",
  "repository": {
    "type": "git",
